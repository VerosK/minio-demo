{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Action": [
        "s3:PutObject",
        "s3:GetObject",
        "s3:ListBucket",
        "s3:DeleteObject"
      ],
      "Effect": "Allow",
      "Resource": [
        "arn:aws:s3:::data/*",
        "arn:aws:s3:::cache/*",
        "arn:aws:s3:::public/*"
      ],
      "Sid": "EnableAccess"
    }
  ]
}
